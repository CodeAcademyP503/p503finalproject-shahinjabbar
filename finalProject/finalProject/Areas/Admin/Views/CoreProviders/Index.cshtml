@model IEnumerable<finalProject.Models.CoreProvider>

@{
    ViewData["Title"] = "Index";
    Layout = "/Areas/Admin/Views/Shared/_Layout.cshtml";
}



<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TagName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BackColor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagePath)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Text)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LangId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TagName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BackColor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagePath)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Text)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LangId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" asp-controller="CoreProviders">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id" asp-controller="CoreProviders">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id" asp-controller="CoreProviders">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
